    "ReactJS",
    "Open Source",
    "payments",
    "paystack",
    "Gateway"
  ],
  "author": "Olusegun Ayeni <nsegun5@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamraphson/react-paystack/issues"
  },
  "homepage": "https://github.com/iamraphson/react-paystack#readme",
