    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --verbose --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamraphson/react-paystack.git"
  },
  "keywords": [
